<!-- <p id="notice"><%= notice %></p> -->

<p>
  <strong>Event:</strong>
  <%= @post.event.title %>
</p>

<p>
  <strong>Text content:</strong>
  <%= @post.text_content %>
</p>

<p>
  <strong>Vote count:</strong>
  <%= @post.vote_count %>
</p>

<p>
  <strong>Post at:</strong>
  <%= @post.created_at %>
</p>
<div class="table-responsive">
    <table class="table table-striped">
        <thead>
            <th>Users</th>
        </thead>
        <tbody>
            <% @users.each do |user| %>
            <tr>
                <td><%= user.email %></td>
            </tr>
            <% end %>
        </tbody>
    </table>
</div>

<%= button_to "vote", action: "vote", params: {from: 2}%>
<%= link_to 'Edit', edit_post_path(@post) %> |
<%= link_to 'Back', event_path(@post.event) %>
