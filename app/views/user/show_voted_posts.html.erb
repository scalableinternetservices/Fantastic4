<p id="notice"><%= notice %></p>

<h1 class="sub-header">Posts voted by me</h1>

<div class="table-responsive">
    <table class="table table-striped">
        <thead>
			<th>Post content</th>
			<th>Current vote</th>
        </thead>
        <tbody>
			<% @posts.each do |post| %>
					<tr>
						<td><%= link_to "#{post.text_content}", post%></td>
						<td><%= post.vote_count%></td>
						
					</tr>
			<% end %>
        </tbody>
    </table>
</div>

<button onclick="window.history.back()">Back</button>
