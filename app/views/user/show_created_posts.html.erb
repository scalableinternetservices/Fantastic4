<p id="notice"><%= notice %></p>

<h1 class="sub-header">Posts created by me</h1>

<div class="table-responsive">
    <table class="table table-striped">
        <thead>
            <th>Title</th>
			<th>Event</th>
			<th>Total vosts</th>
			<th>Create Time</th>
        </thead>
        <tbody>
            <% @created_posts.each do |post| %>
            <tr>
                <td><%= link_to "#{post.text_content}", post%></td>
				<td><%= link_to "#{post.event.title}", post.event%></td>
				<td><%= post.vote_count%></td>
				<td><%= post.created_at%></td>
				<td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
            <% end %>
        </tbody>
    </table>
</div>



